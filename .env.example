SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
HELIUS_RPC_URL=
ENABLE_TOKEN_METADATA=true
ORCA_RPC_URL=
ORCA_RPC_URLS=
ORCA_RPC_RETRY_ATTEMPTS=3
ORCA_RPC_RETRY_BASE_MS=250

# Jupiter Portfolio API key (required for Jupiter Perps details).
JUPITER_API_KEY=

# Optional fallback key used for backwards compatibility.
POSITIONS_API_KEY=

# Optional Jupiter endpoint override.
JUPITER_PORTFOLIO_ENDPOINT=https://api.jup.ag/portfolio/v1/positions/{wallet}

# Optional legacy perps endpoint overrides (comma-separated templates with {wallet}).
JUPITER_PERPS_ENDPOINTS=

# Kamino API settings.
KAMINO_BASE_URL=https://api.kamino.finance
KAMINO_ENV=mainnet-beta

# Optional comma-separated RPCs for Kamino on-chain SDK lookup (first success wins).
KAMINO_SDK_RPC_URLS=https://api.mainnet-beta.solana.com
